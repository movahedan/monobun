{
	"private": false,
	"name": "@repo/intershell",
	"version": "0.2.0",
	"description": "Core utilities and types for the InterShell CLI framework",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"./core": "./src/core/index.ts",
		"./entities": "./src/entities/index.ts"
	},
	"files": [
		"dist",
		"src",
		"README.md"
	],
	"packageManager": "bun@1.2.21",
	"scripts": {
		"check:types": "tsc --noEmit",
		"build": "tsc",
		"prebuild": "bun run ../../scripts/update-package-json.ts"
	},
	"devDependencies": {
		"@repo/test-preset": "workspace:*",
		"@repo/typescript-config": "workspace:*"
	},
	"peerDependencies": {
		"@biomejs/biome": "2.1.2",
		"lefthook": "1.12.3",
		"turbo": "2.5.6",
		"typescript": "5.9.2"
	},
	"keywords": [
		"cli",
		"interactive",
		"terminal",
		"framework",
		"pages",
		"navigation",
		"state-management",
		"command-parser",
		"plugin-system",
		"entities",
		"commit-management",
		"branch-validation"
	]
}
