{
	"name": "@repo/ui",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"sideEffects": false,
	"files": [
		"dist/**",
		"dist"
	],
	"exports": {
		"./counter-button": {
			"import": {
				"types": "./dist/es/counter-button.d.mts",
				"default": "./dist/es/counter-button.mjs"
			},
			"require": {
				"types": "./dist/cjs/counter-button.d.ts",
				"default": "./dist/cjs/counter-button.js"
			}
		},
		"./link": {
			"import": {
				"types": "./dist/es/link.d.mts",
				"default": "./dist/es/link.mjs"
			},
			"require": {
				"types": "./dist/cjs/link.d.ts",
				"default": "./dist/cjs/link.js"
			}
		}
	},
	"scripts": {
		"build": "bunchee",
		"dev": "bunchee --watch",
		"start": "bunchee",
		"check-types": "tsc --noEmit",
		"test": "jest"
	},
	"jest": {
		"preset": "@repo/jest-presets/browser"
	},
	"devDependencies": {
		"@jest/globals": "29.7.0",
		"@repo/jest-presets": "*",
		"@repo/typescript-config": "*",
		"bunchee": "6.5.1",
		"jest": "29.7.0",
		"typescript": "5.8.3"
	},
	"peerDependencies": {
		"@types/react": "18.3.21",
		"@types/react-dom": "18.3.7",
		"react": "18.3.1",
		"react-dom": "18.3.1"
	}
}
